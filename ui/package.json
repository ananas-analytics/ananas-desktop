  "main": "dist/app.js",
  "scripts": {
    "test": "jest",
    "test-dev": "jest --watch",
    "start": "electron .",
    "dev-ui": "export NODE_ENV=development && webpack-dev-server --mode development",
    "webpack": "export NODE_ENV=development && webpack --watch --mode development",
    "webpack-prod": "export NODE_ENV=production && webpack --mode production",
    "webpack-main": "export NODE_ENV=development && webpack --config webpack.main.config.js --watch --mode development",
    "webpack-main-prod": "export NODE_ENV=production && webpack --config webpack.main.config.js --mode production",
    "pack": "electron-builder --dir",
    "dist-mac": "export NODE_ENV=production && yarn run webpack-prod && yarn run webpack-main-prod && electron-builder",
    "dist-win": "export NODE_ENV=production && yarn run webpack-prod && yarn run webpack-main-prod && electron-builder --win",
    "dist-linux": "export NODE_ENV=production && yarn run webpack-prod && yarn run webpack-main-prod && electron-builder --linux"
  },
  "author": "Bo HOU <daily.bhou@gmail.com>",
