{
  "name": "AnanasAnalytics",
  "description": "Analytics Made Easy",
  "version": "0.6.0",
  "main": "dist/app.js",
  "scripts": {
    "test": "jest",
    "test-dev": "jest --watch",
    "start": "electron .",
    "dev-ui": "export NODE_ENV=development && webpack-dev-server --mode development",
    "webpack": "export NODE_ENV=development && webpack --watch --mode development",
    "webpack-prod": "export NODE_ENV=production && webpack --mode production",
    "webpack-main": "export NODE_ENV=development && webpack --config webpack.main.config.js --watch --mode development",
    "webpack-main-prod": "export NODE_ENV=production && webpack --config webpack.main.config.js --mode production",
    "pack": "electron-builder --dir",
    "dist-mac": "export NODE_ENV=production && yarn run webpack-prod && yarn run webpack-main-prod && electron-builder",
    "dist-win": "export NODE_ENV=production && yarn run webpack-prod && yarn run webpack-main-prod && electron-builder --win",
    "dist-linux": "export NODE_ENV=production && yarn run webpack-prod && yarn run webpack-main-prod && electron-builder --linux"
  },
  "author": "Bo HOU <daily.bhou@gmail.com>",
  "build": {
    "appId": "com.ananasanalytics.desktop",
    "productName": "Ananas Analytics Desktop Edition",
    "copyright": "Copyright Â© 2019 Ananas Analytics https://ananasanalytics.com",
    "directories": {
      "output": "../bin"
    },
    "files": [
      "dist/app.js",
      "dist/bundle.js",
      "build/*",
      "build/icons/*",
      "public/*",
      "public/images/*"
    ],
    "extraResources": [
      "resources/**/*"
    ],
    "mac": {
      "category": "public.app-category.business"
    },
    "linux": {
      "category": "Office"
    },
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "del": "^4.1.1",
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "eslint": "^5.12.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.91.0",
    "flow-webpack-plugin": "^1.2.0",
    "jest": "^24.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "tmp": "^0.1.0",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-conditional-loader": "^1.0.12",
    "webpack-dev-server": "3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bson-objectid": "^1.2.4",
    "codemirror": "^5.43.0",
    "d3-array": "^2.0.3",
    "dagre": "^0.8.4",
    "deepmerge": "^3.1.0",
    "electron-log": "^3.0.1",
    "graphlib": "^2.1.7",
    "grommet": "^2.7.0",
    "grommet-icons": "^4.2.0",
    "grommet-theme-v1": "^0.1.2",
    "jsplumb": "^2.9.3",
    "lokijs": "^1.5.6",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "normalize-path": "^3.0.0",
    "panzoom": "^7.1.2",
    "react": "16.8.4",
    "react-codemirror": "^1.0.0",
    "react-color": "^2.17.0",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "16.8.4",
    "react-dropzone": "^9.0.0",
    "react-google-charts": "^3.0.13",
    "react-redux": "^6.0.0",
    "react-resize-detector": "^4.2.0",
    "react-table": "^6.9.0",
    "readdirp": "^3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "yaml": "^1.3.2"
  }
}
